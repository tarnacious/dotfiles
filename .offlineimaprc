[general]
accounts = Private, Seedad, debugproxy
pythonfile=~/.mutt/offlineimap-pass.py

[Account Private]
localrepository = LocalPrivate
remoterepository = RemotePrivate

[Account Seedad]
localrepository = LocalSeedad
remoterepository = RemoteSeedad

[Account debugproxy]
localrepository = LocalDebugProxy
remoterepository = RemoteDebugProxy

[Repository LocalSeedad]
type = Maildir
localfolders = ~/Mail/seedad

[Repository LocalPrivate]
type = Maildir
localfolders = ~/Mail/private

[Repository LocalDebugProxy]
type = Maildir
localfolders = ~/Mail/debugproxy

